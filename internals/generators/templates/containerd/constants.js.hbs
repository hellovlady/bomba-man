import { APP_NAME } from 'settings';

// Defaults
export const KEY = '{{properCase name}}';
{{#if wantAsync}}
	export const FETCH_URL = '/';
{{/if}}

// Types
{{#if wantAsync}}
export const FETCH_DATA_REQUEST = `${APP_NAME}/${KEY}/FETCH_DATA_REQUEST`;
export const FETCH_DATA_SUCCESS = `${APP_NAME}/${KEY}/FETCH_DATA_SUCCESS`;
export const FETCH_DATA_ERROR = `${APP_NAME}/${KEY}/FETCH_DATA_ERROR`;
{{else}}
export const SET_{{ constantCase defaultStateName }} = `${APP_NAME}/${KEY}/SET_{{ constantCase defaultStateName }}`;
{{/if}}
